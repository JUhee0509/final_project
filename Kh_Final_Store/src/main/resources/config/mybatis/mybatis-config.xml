<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.ProductVO" alias="prod"/>
		
		<typeAlias type="vo.AddrVO" alias="addr"/>
		<typeAlias type="vo.AdministratorVO" alias="administrator"/>
		<typeAlias type="vo.BlacklistVO" alias="blacklist"/>
		<typeAlias type="vo.CartVO" alias="cart"/>
		<typeAlias type="vo.ConsumerVO" alias="consumer"/>
		<typeAlias type="vo.FaqVO" alias="faq"/>
		<typeAlias type="vo.InquirecommentVO" alias="inqcomm"/>
		<typeAlias type="vo.InquireVO" alias="inquire"/>
		<typeAlias type="vo.MdVO" alias="md"/>
		<typeAlias type="vo.NoticeVO" alias="notice"/>
		<typeAlias type="vo.OrderVO" alias="order"/>
		<typeAlias type="vo.OrderdetailVO" alias="orderdetail"/>
		<typeAlias type="vo.PickVO" alias="pick"/>
		<typeAlias type="vo.ProductVO" alias="product"/>
		<typeAlias type="vo.ReviewcommentVO" alias="reviewcomment"/>
		<typeAlias type="vo.ReviewVO" alias="review"/>
		<typeAlias type="vo.ReceiptVO" alias="receipt"/>
		
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/addr.xml" />
		<mapper resource="config/mybatis/mapper/administrator.xml" />
		<mapper resource="config/mybatis/mapper/blacklist.xml" />
		<mapper resource="config/mybatis/mapper/cart.xml" />
		<mapper resource="config/mybatis/mapper/consumer.xml" />
		<mapper resource="config/mybatis/mapper/faq.xml" />
		<mapper resource="config/mybatis/mapper/inquirecomment.xml" />
		<mapper resource="config/mybatis/mapper/inquire.xml" />
		<mapper resource="config/mybatis/mapper/md.xml" />
		<mapper resource="config/mybatis/mapper/notice.xml" />
		<mapper resource="config/mybatis/mapper/order.xml" />
		<mapper resource="config/mybatis/mapper/orderdetail.xml" />
		<mapper resource="config/mybatis/mapper/pick.xml" />
		<mapper resource="config/mybatis/mapper/product.xml" />
		<mapper resource="config/mybatis/mapper/reviewcomment.xml" />
		<mapper resource="config/mybatis/mapper/review.xml" />
		<mapper resource="config/mybatis/mapper/receipt.xml" />
	</mappers>
</configuration>